<script src="https://cdn.jsdelivr.net/npm/velocity-animate@1.5.0/velocity.min.js"></script>
<script>
    $(document).ready(function () {
        var temp;
        var properties = null
        $('.indexelepic').tooltip();
        
            



        $("a[class^='Main_listele']").click(function () {
            if ($(this)[0] === temp) {
                if ($('.mapmove').hasClass('clicked')) {
                    properties = {
                        'margin-right': '15%',
                        'margin-left': '16%'
                    };
                    temp = $(this)[0];
                    $('.mapmove').removeClass('clicked')


                } else {
                    properties = {
                        'margin-left': '2%',
                        'margin-right': '29%'
                    };
                    temp = $(this)[0]
                    $('.mapmove').addClass('clicked')

                }
            } else {
                properties = {
                    'margin-left': '2%',
                    'margin-right': '29%'

                };
                $('.mapmove').addClass('clicked')
                temp = $(this)[0];
            }
            //velocity
            $('.mapmove').stop().velocity(properties, 500)
        });

    });

</script>