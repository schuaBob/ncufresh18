<script>
    $(document).ready(function () {
        
        console.log($('#map').height())
        var temp;
        var properties = null
        $("a[class^='Main_listele']").click(function () {
            if ($(this)[0] === temp) {
                if ($('#map').hasClass('clicked')) {
                    properties = {
                        'margin-left': '15%'
                    };
                    temp = $(this)[0];
                    $('#map').removeClass('clicked')
                    

                } else {
                    properties = {
                        'margin-left': '2%'
                    };
                    temp = $(this)[0]
                    $('#map').addClass('clicked')
                    
                }
            } else {
                properties = {
                    'margin-left': '2%'
                };
                $('#map').addClass('clicked')
                temp = $(this)[0];
            }
            //velocity
            $('#map').stop().animate(properties, 500)
        });
        
    });

</script>