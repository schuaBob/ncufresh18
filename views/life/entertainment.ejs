<!DOCTYPE html>
<html>
<head>
    <% include ../layout/head %>
    <link rel=stylesheet type="text/css" href="../life/life.css">
</head>
<body> 
    <header>
        <% include ../layout/navbar %>
    </header>
    <div class="header-2"></div>
    <div class="selectArea">
        <% for (var i in life) { %>
            <% if (life[i].type == "entertain") { %>
                <div class="selectbar">
                    <div id="circle_little"></div>
                    <p class="mainTitle" id="1_<%=i%>"> <%= life[i].mainTitle %></p>
                    <% if(life[i].subTitle.length != 0) { %>
                        <hr class="hide" id="1_<%=i%>_hr"/>
                        <ul class="hide" id="1_<%=i%>_sub">
                        <% for (var j=0 ; j<life[i].subTitle.length ; j++) { %>
                            <li><a href=""><%= life[i].subTitle[j] %></a></li>
                        <% } %>
                        </ul>
                    <% } %>
                </div>
            <% } %>
        <% } %>
    </div>
    <div class="pictureArea">
    </div>
    <div class="discribeArea">
    </div>
    <% include navi %>
    <footer>
        <% include ../layout/footer %>
    </footer>
        <% include ../layout/js %>
        <% include js %>
</body>
</html>
