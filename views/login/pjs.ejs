<script>
    var url = new URL(window.location.href);
    document.getElementsByName('password')[0].focus() ;
    $("input[name=id]").val(url.searchParams.get('id'))// = url.searchParams.get('id');
    $(document).on("keypress",function(event){
        if(event.keyCode === 13 ){
            $("form:first").submit();
        }
    });
    $(document).on("click","#btn",function(event){
        $("form:first").submit(); 
    });
    $("#num").popover({
        content: "密碼有誤" 
    })
    $("#num").popover('show') 
    $("#num").focus() ;
</script>